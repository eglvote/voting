{
  "name": "egl",
  "version": "1.0.0",
  "description": "",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "truffle test --network ganache",
    "compile": "truffle compile --all",
    "mig-ganache": "truffle migrate --reset --network ganache",
    "mig-ganache-token": "truffle migrate -f 2 --to 2 --reset --network ganache",
    "mig-ganache-voting": "truffle migrate -f 3 --to 3 --reset --network ganache",
    "mig-ropsten": "truffle migrate --reset --network ropsten --skipDryRun",
    "mig-ropsten-token": "truffle migrate -f 2 --to 2 --reset --network ropsten --skipDryRun",
    "mig-ropsten-voting": "truffle migrate -f 3 --to 3 --reset --network ropsten --skipDryRun",
    "con-ganache": "truffle console --network ganache",
    "con-ropsten": "truffle console --network ropsten",
    "size": "truffle run contract-size",
    "auto-tally-ganache": "truffle exec test/helpers/auto_tally_votes.js  --network=ganache",
    "auto-tally-ropsten": "truffle exec test/helpers/auto_tally_votes.js  --network=ropsten"
  },
  "author": "Shane van Coller",
  "license": "MIT",
  "devDependencies": {
    "@openzeppelin/contracts-upgradeable": "^3.3.0",
    "@openzeppelin/test-helpers": "^0.5.10",
    "@openzeppelin/truffle-upgrades": "^1.5.0",
    "@truffle/contract": "^4.3.5",
    "@truffle/hdwallet-provider": "^1.2.1",
    "bn.js": "^5.1.3",
    "dotenv": "^8.2.0",
    "eth-gas-reporter": "^0.2.21",
    "solhint": "^3.3.2",
    "truffle-contract-size": "^2.0.1"
  }
}
